alias ll="ls -lha"

alias l="git log --pretty=format:'%C(yellow)%h %Cblue%>(12)%ad %Cred%d %Creset%s'"
alias lg="git log --oneline | grep -i"
alias commit_l="git log --date=short --pretty=format:'%C(yellow)%h %Cred%ad %Creset%s %C(yellow)%d'"

alias ip="curl 'https://api.ipify.org'"

alias vhosts="cd /etc/apache2/sites-available/"
alias www="cd /var/www/"
